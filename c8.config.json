{
  "$schema": "https://raw.githubusercontent.com/bcoe/c8/main/coverage-schema.json",
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/**/*.test.ts",
    "src/**/*.spec.ts",
    "src/types.ts",
    "dist/**",
    "node_modules/**",
    "coverage/**",
    "examples/**",
    "test/**",
    "scripts/**",
    "**/*.d.ts"
  ],
  "all": true,
  "src": ["src"],
  "reporter": [
    "text",
    "text-summary"
  ],
  "temp-directory": "./coverage/tmp",
  "reports-directory": "./coverage",
  "extension": [
    ".ts"
  ],
  "check-coverage": false,
  "lines": 0,
  "functions": 0,
  "branches": 0,
  "statements": 0
}
