{
  "$schema": "https://raw.githubusercontent.com/Mearman/cairs/main/eir.schema.json",
  "version": "1.0.0",
  "description": "Interactive example: reads a single line of input and outputs it multiple times in different contexts. Demonstrates single input with multiple outputs. Pattern: 1 input → many outputs.",
  "airDefs": [],
  "nodes": [
    {
      "id": "readInput",
      "expr": {
        "kind": "effect",
        "op": "readLine",
        "args": []
      }
    },
    {
      "id": "echoFormat1",
      "expr": {
        "kind": "effect",
        "op": "print",
        "args": [
          "readInput"
        ]
      }
    },
    {
      "id": "echoFormat2",
      "expr": {
        "kind": "effect",
        "op": "print",
        "args": [
          "readInput"
        ]
      }
    },
    {
      "id": "echoFormat3",
      "expr": {
        "kind": "effect",
        "op": "print",
        "args": [
          "readInput"
        ]
      }
    },
    {
      "id": "seq12",
      "expr": {
        "kind": "seq",
        "first": "echoFormat1",
        "then": "echoFormat2"
      }
    },
    {
      "id": "finalSeq",
      "expr": {
        "kind": "seq",
        "first": "seq12",
        "then": "echoFormat3"
      }
    }
  ],
  "result": "finalSeq",
  "note": "Interactive example: reads one line of input and echoes it three times. Demonstrates the 1 input → many outputs pattern."
}
